extends ../layout

block content
    h4.pt-3
    img(src='https://discoduroexterno.shop/wp-content/uploads/2019/09/western-digital-logo.jpg' style='width: 170px; height: 150px;')
    img(src='data:image/png;base64,' style='width: 170px; height: 150px;')
    img(src='https://assets.change.org/photos/0/hk/md/gWhkmDtNhPnOlOg-400x400-noPad.jpg?1522276920' style='width: 170px; height: 150px;')
    img(src='data:image/png;base64,' style='width: 170px; height: 150px;')
    img(src='https://www.madboxpc.com/wp-content/uploads/2019/01/asus-logo-negroddd.png' style='width: 170px; height: 150px;')
    img(src='https://tecnoplaza.com.co/sitio/wp-content/uploads/2017/09/msi-logo.jpg' style='width: 170px; height: 150px;')
    img(src='https://media.licdn.com/dms/image/C4D0BAQEcAykrxbb4oA/company-logo_200_200/0?e=2159024400&v=beta&t=cvjBSbBGgbmYBCQGYtvRh_gNWNQG2AGr5t0gASr5XEw' style='width: 170px; height: 150px;')
    br
    br
    h4.text-center Asistente de armado de pc
    br
    a.btn.btn-info(href='/equipos/create') Elegir componentes
    a.btn.btn-danger(href='/') Volver a la pagina principal
    a.btn.btn-secondary(href='/login') Loguearse
    table.table
        thead
         tr
            th(scope='col') #
            th(scope='col') Procesador
            th(scope='col') Fuente
            th(scope='col') Placa de Video
            th(scope='col') Motherboard
            th(scope='col') Almacenamiento
            th(scope='col') Memoria Ram
            th(scope='col') Gabinete
            th(scope='col') Disipador
            th(scope='col') 
            th(scope='col') 
         tbody
         tr
          each equipo in equipos
           th(scope='row')= equipo.id
           td=equipo.procesador
           td=equipo.fuente
           td=equipo.graphics
           td=equipo.motherboard
           td=equipo.almacenamiento
           td=equipo.memoriaram
           td=equipo.gabinete
           td=equipo.disipador
           td
            a(href='equipos/'+equipo.id+'/update') Editar
           td
               form.inline(method='post' action='equipos/' + equipo.id + '/delete')
                 input(type='hidden' name='id' value=equipo.id)
                 button.link-button(type='submit' name='submit_param' value='submit_value' class='btn btn-danger')='Eliminar'
           tr
           br
           br
           br
